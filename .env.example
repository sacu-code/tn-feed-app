# Tiendanube client ID for your app (from the Partner portal)
TN_CLIENT_ID=your_client_id_here

# Tiendanube client secret (from the Partner portal)
TN_CLIENT_SECRET=your_client_secret_here

# Base URL where your app is hosted. In Vercel this will be something like
# https://your-project-name.vercel.app
APP_URL=https://your-app-url.example.com

# API version. Use v1 or a specific date like 2025-03. Default: v1
API_VERSION=v1

# A random secret used for signing sessions or JWTs if you extend this app
JWT_SECRET=change_me_secret_key